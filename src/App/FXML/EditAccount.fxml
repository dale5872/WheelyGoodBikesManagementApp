<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane styleClass="main-background" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.FXControllers.EditAccountController">
   <children>
      <VBox layoutX="20.0" layoutY="20.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
               <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                   <children>
                       <Label fx:id="allFieldsWarning" text="All fields are required" textFill="WHITE">
                           <font>
                               <Font size="14.0" />
                           </font>
                           <VBox.margin>
                               <Insets />
                           </VBox.margin>
                       </Label>
                  <VBox>
                     <children>
                        <Label text="Username" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                             <TextField fx:id="username" layoutX="14.0" layoutY="39.0" prefHeight="30.0">
                                 <font>
                                     <Font size="14.0" />
                                 </font>
                                 <VBox.margin>
                                     <Insets />
                                 </VBox.margin>
                             </TextField>
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Name" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                             <HBox alignment="CENTER" spacing="10.0">
                                 <children>
                                     <TextField fx:id="firstName" prefHeight="30.0" HBox.hgrow="ALWAYS">
                                         <font>
                                             <Font size="14.0" />
                                         </font>
                                         <HBox.margin>
                                             <Insets />
                                         </HBox.margin>
                                     </TextField>
                                     <TextField fx:id="lastName" prefHeight="30.0" HBox.hgrow="ALWAYS">
                                         <font>
                                             <Font size="14.0" />
                                         </font>
                                     </TextField>
                                 </children>
                                 <VBox.margin>
                                     <Insets />
                                 </VBox.margin>
                             </HBox>
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Email" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                             <TextField fx:id="email" prefHeight="30.0">
                                 <font>
                                     <Font size="14.0" />
                                 </font>
                                 <VBox.margin>
                                     <Insets />
                                 </VBox.margin>
                             </TextField>
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <Label text="Phone number" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                             <TextField fx:id="phoneNumber" prefHeight="30.0">
                                 <font>
                                     <Font size="14.0" />
                                 </font>
                                 <VBox.margin>
                                     <Insets />
                                 </VBox.margin>
                             </TextField>
                     </children>
                  </VBox>
                  <HBox spacing="10.0">
                     <children>
                        <VBox HBox.hgrow="ALWAYS">
                           <children>
                              <Label text="Account type" textFill="WHITE">
                                 <font>
                                    <Font name="System Bold" size="14.0" />
                                 </font>
                              </Label>
                                     <ComboBox fx:id="accountTypeCombo" prefHeight="30.0" prefWidth="200.0" promptText="Select..." HBox.hgrow="ALWAYS">
                                         <HBox.margin>
                                             <Insets />
                                         </HBox.margin>
                                     </ComboBox>
                           </children>
                        </VBox>
                        <VBox fx:id="workLocationBox" HBox.hgrow="ALWAYS">
                           <children>
                              <Label text="Work location" textFill="WHITE">
                                 <font>
                                    <Font name="System Bold" size="14.0" />
                                 </font>
                              </Label>
                                     <ComboBox fx:id="workLocationCombo" prefHeight="30.0" prefWidth="200.0" promptText="Select..." HBox.hgrow="ALWAYS">
                                         <HBox.margin>
                                             <Insets />
                                         </HBox.margin>
                                     </ComboBox>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                   </children>
               </VBox>
                 <HBox alignment="CENTER_RIGHT" spacing="10.0">
                     <children>
                         <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#confirm" prefWidth="100.0" styleClass="green-button" text="Update" textOverrun="CLIP" />
                  <Button mnemonicParsing="false" onAction="#close" prefWidth="100.0" styleClass="green-button" text="Cancel" textOverrun="CLIP" />
                     </children>
                 </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</AnchorPane>
